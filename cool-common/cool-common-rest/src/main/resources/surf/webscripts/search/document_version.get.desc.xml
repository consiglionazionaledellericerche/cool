<webscript>
  <shortname>Document version</shortname>
  <description>
  <![CDATA[
  Executes a CMIS query statement against the contents of the Repository.
  ]]>
  </description>
  <url>/search/document/version?nodeRef={nodeRef}</url>
  <args>
    <arg>
        <shortname>nodeRef</shortname>
        <description>nodeRef of document</description>
    </arg>
  </args>
  <format default="json"/> 
  <authentication>guest</authentication>
  <family>SEARCH</family>
</webscript>