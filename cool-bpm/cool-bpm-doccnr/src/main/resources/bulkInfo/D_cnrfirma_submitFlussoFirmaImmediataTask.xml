<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<bulkInfo xmlns="http://www.cnr.it/schema/BulkInfo_v2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" cmisQueryName="cnrfirma:submitFlussoFirmaImmediataTask"
	cmisTypeName="D:cnrfirma:submitFlussoFirmaImmediataTask"
	longDescription="cnrfirma Start FlussoFirmaImmediata Task"
	shortDescription="cnrfirma Start FlussoFirmaImmediata Task"
	xsi:schemaLocation="http://www.cnr.it/schema/BulkInfo_v2 BulkInfo_v2">


	<fieldProperty class="input-xlarge" inputType="TEXTAREA"
		labelClass="control-label" name="bpm:comment" nullable="false"
		property="bpm:comment" rows="3">
		<jsonlabel default="Commento" key="label.bpm.comment" />
	</fieldProperty>


	<fieldProperty name="cm:owner" property="cm:owner"
		visible="false" />

	<fieldProperty inputType="ROTEXT" labelClass="control-label"
		name="wfcnr:groupName" nullable="false" property="wfcnr:groupName"
		value="GROUP_FIRMATARI" >
		<jsonlabel default="Ufficio:" key="label.wfcnr.groupName" />
		<jsonvalidator requiredWidget="true" />
	</fieldProperty>

	<fieldProperty inputType="TEXT" label="Folder di destinazione:"
		name="cnrfirma:nodeRefFolderToLink" placeholder="seleziona il Folder"
		property="cnrfirma:nodeRefFolderToLink" widget="ui.tree">
		<jsonvalidator required="false" />
	</fieldProperty>

	<fieldProperty class="input-xlarge" inputType="TEXT"
		label="id utente" labelClass="control-label" name="cnrfirma:userFirma"
		property="cnrfirma:userFirma">
		<jsonvalidator required="true" />
	</fieldProperty>

	<fieldProperty class="input-xlarge" inputType="TEXT"
		label="pw utente" labelClass="control-label" name="cnrfirma:userPwFirma"
		property="cnrfirma:userPwFirma">
		<jsonvalidator required="true" />
	</fieldProperty>

	<fieldProperty class="input-xlarge" inputType="TEXT"
		label="PIN" labelClass="control-label" name="cnrfirma:pinFirma"
		property="cnrfirma:pinFirma">
		<jsonvalidator required="true" />
	</fieldProperty>

	<fieldProperty class="input-xlarge" inputType="TEXT"
		label="codice Documento" labelClass="control-label" name="cnrfirma:codiceDocumentoUfficio"
		property="cnrfirma:codiceDocumentoUfficio">
		<jsonvalidator required="false" />
	</fieldProperty>
	<!-- formFieldProperty -->
	<!-- Default -->
	<form name="default">
		<formFieldProperty name="wfcnr:groupName" />
		<formFieldProperty name="bpm:comment" />
		<formFieldProperty name="cnrfirma:nodeRefFolderToLink" />
		<formFieldProperty name="cnrfirma:userFirma" />
		<formFieldProperty name="cnrfirma:userPwFirma" />
		<formFieldProperty name="cnrfirma:pinFirma" />
		<formFieldProperty name="cnrfirma:codiceDocumentoUfficio"/>

	</form>

	<!-- Start -->
	<form name="Start">
		<formFieldProperty name="wfcnr:groupName" />
		<formFieldProperty name="bpm:comment" />
		<formFieldProperty name="cnrfirma:nodeRefFolderToLink" />
		<formFieldProperty name="cnrfirma:userFirma" />
		<formFieldProperty name="cnrfirma:userPwFirma" />
		<formFieldProperty name="cnrfirma:pinFirma" />
		<formFieldProperty name="cnrfirma:codiceDocumentoUfficio" />
	</form>

</bulkInfo>