<bulkInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.cnr.it/schema/BulkInfo" xsi:schemaLocation="http://www.cnr.it/schema/BulkInfo BulkInfo.xsd"
	shortDescription="wfcnr Proroga Bando" longDescription="wfcnr Proroga Bando"
	cmisTypeName="D:wfcnr:prorogaBandoTask" cmisQueryName="wfcnr:prorogaBandoTask">

	<fieldProperty
		labelClass="control-label"
		visible="true"
		jsonlist='[{
				"defaultLabel" : "Approva Proroga Bando",
				"label" : "Approva Proroga Bando",
				"key" : "Approve"
			}, {
				"defaultLabel" : "Rifiuta Proroga Bando",
				"label" : "Rifiuta Proroga Bando",
				"key" : "Reject"
			}
		]'
		name="wfcnr:reviewOutcome"
		jsonlabel='{
			"key" : "label.wfcnr.reviewOutcome",
			"default" : "Prosegui"
		}'
		inputType="SELECT"
		property="wfcnr:reviewOutcome"
		widget="ui.select"/>

		 		
	<fieldProperty
		name="bpm:comment"
		property="bpm:comment"
		label="label.bpm.comment"
		inputType="TEXTAREA"
		rows="3"
		class="input-xlarge"
		labelClass="control-label"
		nullable="false" />
		
	<fieldProperty
		name="wfcnr:dataTask1"
		property="wfcnr:dataTask1"
		label="label.wfcnr.dataTask1"
		placeholder="placeholder.wfcnr.dataTask1"
		inputType="TEXT"
		type="date"
		class="input-xlarge"
		labelClass="control-label"
		widget="ui.datetimepicker"
		nullable="false" />
		
		
	<formFieldProperty name="wfcnr:reviewOutcome" />
	<formFieldProperty name="wfcnr:dataTask1" />
	<formFieldProperty name="bpm:comment" />
		
		<!-- FORM -->
	<form name="Approve">
		<formFieldProperty name="wfcnr:dataTask1" />
		<formFieldProperty name="bpm:comment" />
	</form>
	
	<form name="Reject">
		<formFieldProperty name="bpm:comment" />
	</form>
</bulkInfo>