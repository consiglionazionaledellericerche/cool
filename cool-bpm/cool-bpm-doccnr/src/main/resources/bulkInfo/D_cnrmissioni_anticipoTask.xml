		<jsonlist>
			<listElement defaultLabel="Conferma Modifica" key="Done" label="Conferma Modifica"/>
		</jsonlist>
	</fieldProperty>
	
	<fieldProperty
		default="1"
		inputType="RADIOGROUP"
		label="Tipologia Trasporto"
		labelClass="control-label"
		name="cnrmissioni:tipologiaTrasporto"
		nullable="false"
		property="cnrmissioni:tipologiaTrasporto"
		type="buttonset"
		widget="ui.radio">
		<jsonlist>
			<listElement defaultLabel="taxi" key="taxi" label="taxi"/>
			<listElement defaultLabel="noleggio" key="noleggio" label="noleggio"/>
			<listElement defaultLabel="auto di servizio" key="autoservizio" label="auto di servizio"/>
			<listElement defaultLabel="auto propria" key="autopropria" label="auto propria"/>
		</jsonlist>
	</fieldProperty>
	
	<fieldProperty
		default="1"
		inputType="RADIOGROUP"
		label="Autorizzazione Anticipo"
		labelClass="control-label"
		name="cnrmissioni:autorizzazioneAnticipo"
		nullable="false"
		property="cnrmissioni:autorizzazioneAnticipo"
		type="buttonset"
		widget="ui.radio">
		<jsonlist>
			<listElement defaultLabel="autorizzato" key="autorizzato" label="autorizzato"/>
			<listElement defaultLabel="rifiutato" key="rifiutato" label="rifiutato"/>
