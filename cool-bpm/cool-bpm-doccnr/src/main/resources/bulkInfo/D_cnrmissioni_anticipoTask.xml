<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<bulkInfo xmlns="http://www.cnr.it/schema/BulkInfo_v2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" cmisQueryName="cnrmissioni:anticipoTask" cmisTypeName="D:cnrmissioni:anticipoTask" longDescription="cnrmissioni anticipoTask" shortDescription="cnrmissioni anticipoTask" xsi:schemaLocation="http://www.cnr.it/schema/BulkInfo_v2 BulkInfo_v2">

