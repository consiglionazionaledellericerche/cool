	<fieldProperty class="input-xlarge" inputType="HIDDEN" name="nrDocPrincipaliUpload" nullable="false" property="nrDocPrincipaliUpload" val="0" />
	<fieldProperty class="input-xlarge" inputType="HIDDEN" name="nrDocAllegatiiUpload" nullable="false" property="nrDocAllegatiiUpload" val="0" />

	<formFieldProperty name="wfcnr:reviewOutcome"/>
	<formFieldProperty name="bpm:comment"/>

	<!-- FORM -->
	<form name="Done">
		<formFieldProperty name="cnrmissioni:tipologiaTrasporto"/>
		<formFieldProperty name="cnrmissioni:autorizzazioneAnticipo"/>
		<formFieldProperty name="bpm:comment"/>
		<formFieldProperty name="nrDocPrincipaliUpload"/>
		<formFieldProperty name="nrDocAllegatiiUpload"/>
	</form>	

</bulkInfo>