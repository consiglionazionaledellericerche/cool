<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<bulkInfo xmlns="http://www.cnr.it/schema/BulkInfo_v2"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" cmisQueryName="sigla_contabili_aspect:folder"
	cmisTypeName="P:sigla_contabili_aspect:folder" longDescription="Contabili"
	shortDescription="Contabili" xsi:schemaLocation="http://www.cnr.it/schema/BulkInfo_v2 BulkInfo_v2">

	<form name="all-filters">
		<formFieldProperty class="criteria-eq input-small"
			inputType="TEXT" label="esercizio.label" name="esercizio"
			property="contabili.sigla_contabili_aspect:esercizio">
			<jsonvalidator digits="true" />
		</formFieldProperty>
		<formFieldProperty class="criteria-equals input-small"
			inputType="TEXT" label="cds.label" name="cds"
			property="contabili.sigla_contabili_aspect:cds" />
		<formFieldProperty class="criteria-eq input-medium"
			inputType="TEXT" label="num_mandato.label" name="num_mandato"
			property="contabili.sigla_contabili_aspect:num_mandato">
			<jsonvalidator digits="true" />
		</formFieldProperty>

		<formFieldProperty class="badge badge-important animated flash"
			inputType="DIV" name="data_esecuzione" text="data_esecuzione.label" />

		<formFieldProperty class="criteria-gte" inputType="TEXT"
			label="data_esecuzione.da.label" name="da_data_esecuzione"
			property="contabili.sigla_contabili_aspect:data_esecuzione" widget="ui.datepicker" />
		<formFieldProperty class="criteria-lte" inputType="TEXT"
			label="data_esecuzione.a.label" name="a_data_esecuzione"
			property="contabili.sigla_contabili_aspect:data_esecuzione" widget="ui.datepicker" />

		<formFieldProperty class="badge badge-important animated flash"
			inputType="DIV" name="data_invio" text="data_invio.label" />

		<formFieldProperty class="criteria-gte" inputType="TEXT"
			label="data_esecuzione.da.label" name="da_data_invio" property="doc.cmis:creationDate"
			widget="ui.datepicker" />
		<formFieldProperty class="criteria-lte" inputType="TEXT"
			label="data_esecuzione.a.label" name="a_data_invio" property="doc.cmis:creationDate"
			widget="ui.datepicker" />

	</form>

</bulkInfo>