<?xml version="1.0" encoding="UTF-8"?>
	<!-- Definition of new Model -->
<model name="sigla_contabili_aspect:sigla_contabili_aspectModel" 
	xmlns="http://www.alfresco.org/model/dictionary/1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.alfresco.org/model/dictionary/1.0 modelSchema.xsd">

	<!-- Optional meta-data about the model -->
	<description>Contabili extension Content Model</description>
	<author>Marco Spasiano</author>
	<version>1.2</version>

	<!--
		Imports are required to allow references to definitions in other
		models
		Versione 1.2 Aggiunta la property sigla_contabili_aspect:data_esecuzione
	-->
	<imports>
		<!-- Import Alfresco Dictionary Definitions -->
		<import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d" />
		<!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
	</imports>

	<!-- Introduction of new namespaces defined by this model -->
	<namespaces>
		<namespace uri="http://www.cnr.it/model/sigla/contabili/aspect/cmis" prefix="sigla_contabili_aspect" />
	</namespaces>
	<aspects>
		<aspect name="sigla_contabili_aspect:folder">
			<title>Folder degli Istituti per le Conatbili BNL</title>
			<properties>
				<property name="sigla_contabili_aspect:codice_proteo">
					<title>Codice proteo del CdS</title>
					<type>d:text</type>
				</property>
			</properties>
		</aspect>
		<aspect name="sigla_contabili_aspect:discarded_document">
			<title>Conatbili BNL Documento scartato</title>
		</aspect>
		<aspect name="sigla_contabili_aspect:document">
			<title>Conatbili BNL</title>
			<properties>
				<property name="sigla_contabili_aspect:esercizio">
					<title>Esercizio di riferimento</title>
					<type>d:int</type>
				</property>
				<property name="sigla_contabili_aspect:cds">
					<title>CDS della contabile</title>
					<type>d:text</type>
				</property>
				<property name="sigla_contabili_aspect:num_mandato">
					<title>Numero del Mandato</title>
					<type>d:long</type>
				</property>
				<property name="sigla_contabili_aspect:data_esecuzione">
					<title>Data Esecuzione</title>
					<type>d:date</type>
				</property>
			</properties>
		</aspect>
	</aspects>
</model>
