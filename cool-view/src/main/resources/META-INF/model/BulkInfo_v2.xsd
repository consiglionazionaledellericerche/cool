	<xsd:complexType name="FindFieldPropertyType">
		<xsd:complexContent>
			<xsd:extension base="BaseFieldPropertyType">
				<xsd:attribute name="name" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>
							Nome logico del
							FindFieldProperty,l'attributo Ã¨
							obbligatorio, ed deve essere un riferimento
							ad un FieldProperty.
						</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="op" use="optional">
					<xsd:annotation>
						<xsd:documentation>
							Operatore da utilizzare in caso di query cmis
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:NMTOKEN">
							<xsd:enumeration value="contains" />
							<xsd:enumeration value="like" />
							<xsd:enumeration value="lt" />
							<xsd:enumeration value="gt" />
							<xsd:enumeration value="lte" />
							<xsd:enumeration value="gte" />
							<xsd:enumeration value="inFolder" />
							<xsd:enumeration value="inTree" />
							<xsd:enumeration value="equals" />
							<xsd:enumeration value="not" />
							<xsd:enumeration value="and" />
							<xsd:enumeration value="or" />
							<xsd:enumeration value="eq" />
							<xsd:enumeration value="IN" />
							<xsd:enumeration value="notEq" />
							<xsd:enumeration value="isNull" />
							<xsd:enumeration value="isNotNull" />
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="PrintFieldPropertyType">
