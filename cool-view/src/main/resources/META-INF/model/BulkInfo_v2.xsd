	<xsd:complexType name="FormFieldPropertyType">
		<xsd:complexContent>
			<xsd:extension base="BaseFieldPropertyType">
				<xsd:attribute name="name" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>
							Nome logico del FormFieldProperty,
