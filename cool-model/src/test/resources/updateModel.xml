<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="test:model">
	<description>
		Modello di test Contenente due tipi di dato (test:document
		e test2:document)
	</description>
	<author>Paolo Enrico Cirone</author>
	<version>1.0</version>
	<imports>
		<import uri="http://www.alfresco.org/model/dictionary/1.0"
			prefix="d" />
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
	</imports>
	<namespaces>
		<namespace uri="http://www.cnr.it/model/test" prefix="test" />
	</namespaces>
	<types>
		<type name="test:document">
			<title>Documento di test numero 1</title>
			<description>Documento di test numero 1</description>
			<parent>cm:content</parent>
			<properties>
				<property name="test:titoloUpdate">
					<title>Titolo Documento di test numero 1</title>
					<description>Titolo Documento di test numero 1</description>
					<type>d:text</type>
					<mandatory>true</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
				<property name="test:toDeleteUpdate">
					<title>Proprieta' da cancellare</title>
					<description>proprieta' da cancellare</description>
					<type>d:text</type>
					<mandatory>true</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
			</properties>
		</type>
		<type name="test:document2">
			<title>Documento di test numero 2</title>
			<description>Documento di test numero 2</description>
			<parent>cm:content</parent>
			<properties>
				<property name="test:titolo2Update">
					<title>Titolo Documento di test numero 2</title>
					<description>Titolo Documento di test numero 2</description>
					<type>d:text</type>
					<mandatory>true</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
			</properties>
		</type>
	</types>
	<aspects>
		<aspect name="test:aspect">
			<title>
				Aspect di condivisione degli spazi con diversi utenti
			</title>
			<properties>
				<property name="test:newType_1">
					<title>Type</title>
					<description>
						Tipologia del bando a cui ? riferito l'avviso
					</description>
					<type>d:text</type>
					<mandatory>true</mandatory>
					<index enabled="true">
						<atomic>true</atomic>
						<stored>false</stored>
						<tokenised>both</tokenised>
					</index>
				</property>
			</properties>
		</aspect>
	</aspects>
</model>