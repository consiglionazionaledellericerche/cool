<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">

	<rule>
		<from>/</from>
		<to>/foo</to>
	</rule>

	<rule>
		<from>/res/**</from>
		<to>/rest/static/$1</to>
	</rule>

	<rule>
		<from>/search/content</from>
		<to>/rest/content</to>
	</rule>
</urlrewrite>
